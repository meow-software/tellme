{
  "name": "tellme",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "prune": "rimraf prune && npx turbo prune --scope=tellme-back --out-dir=prune/tellme-back --docker",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.5.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@react-email/components": "^0.5.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
